{
  "$schema": "https://opencode.ai/config.json",
  // Theme configuration
  "theme": "system",
  "model": "openai/gpt-5.3-codex",
  "small_model": "openai/gpt-5.1-codex-mini", // NOTE: required! or the opencode-antigravity-auth would fail
  "autoupdate": true,
  "plugin": [
    // "opencode-antigravity-auth@latest"
    "opencode-antigravity-auth@1.4.6"
  ],
  // "plugin": [
  //   // "opencode-antigravity-auth@latest",
  //   // "opencode-antigravity-auth@beta",
  //   "opencode-antigravity-auth@1.4.3",
  //   "oh-my-opencode@beta"
  // ],
  "keybinds": {
    "session_child_cycle": "<leader>right,<leader>l",
    "session_child_cycle_reverse": "<leader>left,<leader>h",
    "session_parent": "<leader>up,<leader>k",
    "tips_toggle": "<leader>t",
    "theme_list": ""
  },
  "permission": {
    "bash": {
      "*": "ask",
      "git *": "allow",
      "git commit *": "deny",
      "git push *": "deny",
      "grep *": "allow",
      "rm *": "ask"
    }
  },
  "agent": {
    "build": {
      "permission": {
        "list": "allow",
        "bash": {
          "git commit *": "ask",
        }
      }
    },
    "explore": {
      "model": "openai/gpt-5.1-codex-mini",
      "permission": {
        "external_directory": "ask",
        "bash": {
          "head": "allow",
          "find *": "allow",
          "find * -exec *": "ask",
          "find * -delete*": "ask"
        }
      }
    },
    "general": {
      // "model": "google/antigravity-claude-sonnet-4-5"
      // "model": "openai/gpt-5.2-codex"
      "model": "openai/gpt-5.3-codex"
    }
  },
  "provider": {
    "openai": {
      "name": "OpenAI",
      "options": {
        "reasoningEffort": "medium",
        "reasoningSummary": "auto",
        "textVerbosity": "medium",
        "include": [
          "reasoning.encrypted_content"
        ],
        "store": false
      }
    },
    "google": {
      "models": {
        "antigravity-gemini-3-pro": {
          "name": "Gemini 3 Pro (Antigravity)",
          "limit": {
            "context": 1048576,
            "output": 65535
          },
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          },
          "variants": {
            "low": {
              "thinkingLevel": "low"
            },
            "high": {
              "thinkingLevel": "high"
            }
          }
        },
        "antigravity-gemini-3-flash": {
          "name": "Gemini 3 Flash (Antigravity)",
          "limit": {
            "context": 1048576,
            "output": 65536
          },
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          },
          "variants": {
            "minimal": {
              "thinkingLevel": "minimal"
            },
            "low": {
              "thinkingLevel": "low"
            },
            "medium": {
              "thinkingLevel": "medium"
            },
            "high": {
              "thinkingLevel": "high"
            }
          }
        },
        "antigravity-claude-sonnet-4-5": {
          "name": "Claude Sonnet 4.5 (Antigravity)",
          "limit": {
            "context": 200000,
            "output": 64000
          },
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          }
        },
        "antigravity-claude-sonnet-4-5-thinking": {
          "name": "Claude Sonnet 4.5 Thinking (Antigravity)",
          "limit": {
            "context": 200000,
            "output": 64000
          },
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          },
          "variants": {
            "low": {
              "thinkingConfig": {
                "thinkingBudget": 8192
              }
            },
            "max": {
              "thinkingConfig": {
                "thinkingBudget": 32768
              }
            }
          }
        },
        "antigravity-claude-opus-4-5-thinking": {
          "name": "Claude Opus 4.5 Thinking (Antigravity)",
          "limit": {
            "context": 200000,
            "output": 64000
          },
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          },
          "variants": {
            "low": {
              "thinkingConfig": {
                "thinkingBudget": 8192
              }
            },
            "max": {
              "thinkingConfig": {
                "thinkingBudget": 32768
              }
            }
          }
        }
